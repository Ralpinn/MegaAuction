                                                        <p>@listNotif[i].contentN</p>
                                                        <p>Product: @listNotif[i].Product.namepro</p>
                                                        @if (listNotif[i].statusNotif == "Fail")
                                                         {
                                                            <a href="~/Home/DetailPro/@listNotif[i].idproduct" style="width: 100px; color: white; font-size: 12px; font-weight: 600;" class="btn btn-danger">Go to product</a>
                                                         }
                                                        else if (listNotif[i].statusNotif == "Success")
